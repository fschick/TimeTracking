{
  "name": "time-tracking",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run build:icons && npm run build:flags && ng serve",
    "start:nr": "npm run start -- --no-live-reload",
    "start:allip": "npm run start -- --host 0.0.0.0 --disable-host-check",
    "start:api": "dotnet run --project ../FS.TimeTracking/FS.TimeTracking/FS.TimeTracking.csproj",
    "build": "npm run build:icons && npm run build:flags && ng build --source-map=true",
    "build:watch": "npm run build -- --watch --configuration development",
    "build:icons": "node icon-generator/create-sprite.js icons",
    "build:flags": "node icon-generator/create-sprite.js flags",
    "lint": "ng lint",
    "test": "ng test --browsers=Chrome",
    "test:single": "npm run test -- --no-watch --browsers=Chrome",
    "test:headless": "npm run test -- --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "test:cypress": "npx cypress run",
    "test:e2e": "start-server-and-test start:api http://localhost:5000/openapi/index.html start 4200 test:cypress",
    "cypress": "npx cypress open"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.9",
    "@angular/common": "~13.3.9",
    "@angular/compiler": "~13.3.9",
    "@angular/core": "~13.3.9",
    "@angular/forms": "~13.3.9",
    "@angular/localize": "~13.3.9",
    "@angular/platform-browser": "~13.3.9",
    "@angular/platform-browser-dynamic": "~13.3.9",
    "@angular/router": "~13.3.9",
    "@ng-bootstrap/ng-bootstrap": "^12.1.2",
    "@ng-select/ng-select": "^8.1.1",
    "apexcharts": "^3.35.2",
    "bootstrap": "^5.1.3",
    "bootstrap-datepicker": "^1.9.0",
    "bootstrap-icons": "^1.8.2",
    "luxon": "^2.4.0",
    "ng-apexcharts": "^1.7.1",
    "ng-let": "^13.0.6",
    "ngx-toastr": "^14.3.0",
    "path-to-regexp": "^6.2.1",
    "rxjs": "~7.5.5",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.6",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "~13.3.6",
    "@angular/compiler-cli": "~13.3.9",
    "@angular/localize": "^13.3.9",
    "@openapitools/openapi-generator-cli": "^2.5.1",
    "@types/bootstrap": "^5.1.12",
    "@types/jasmine": "~4.0.3",
    "@types/jquery": "^3.5.14",
    "@types/luxon": "^2.3.2",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "5.26.0",
    "@typescript-eslint/parser": "5.26.0",
    "bootstrap-icons": "^1.8.2",
    "cypress": "^9.7.0",
    "eslint": "^8.16.0",
    "jasmine-core": "~4.1.1",
    "karma": "~6.3.20",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.1",
    "karma-jasmine-html-reporter": "~1.7.0",
    "start-server-and-test": "^1.14.0",
    "svg-sprite": "^1.5.4",
    "typescript": "~4.6.4"
  }
}
