{
  "name": "time-tracking",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run build:icons && npm run build:flags && ng serve --host 127.0.0.1",
    "start:nr": "npm run start -- --no-live-reload",
    "start:allip": "npm run start -- --host 0.0.0.0 --disable-host-check",
    "start:api": "dotnet run --project ../FS.TimeTracking/FS.TimeTracking/FS.TimeTracking.csproj",
    "build": "npm run build:icons && npm run build:flags && ng build --source-map=true",
    "build:watch": "npm run build -- --watch --configuration development",
    "build:icons": "node icon-generator/create-sprite.js icons",
    "build:flags": "node icon-generator/create-sprite.js flags",
    "lint": "ng lint",
    "cypress": "npx cypress open",
    "test": "npm run test:unit:headless && npm run test:e2e",
    "test:unit": "ng test --browsers=Chrome",
    "test:unit:headless": "npm run test:unit -- --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "test:cypress": "npx cypress run",
    "test:e2e": "start-server-and-test start:api http-get://127.0.0.1:5000/openapi/index.html start http-get://127.0.0.1:4200 test:cypress"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.1.2",
    "@angular/common": "^15.1.2",
    "@angular/compiler": "^15.1.2",
    "@angular/core": "^15.1.2",
    "@angular/forms": "^15.1.2",
    "@angular/localize": "^15.1.2",
    "@angular/platform-browser": "^15.1.2",
    "@angular/platform-browser-dynamic": "^15.1.2",
    "@angular/router": "^15.1.2",
    "@ng-bootstrap/ng-bootstrap": "^14.0.1",
    "@ng-select/ng-select": "^10.0.3",
    "apexcharts": "^3.35.5",
    "bootstrap": "^5.2.0",
    "bootstrap-datepicker": "^1.9.0",
    "bootstrap-icons": "^1.9.1",
    "keycloak-js": "^20.0.3",
    "luxon": "^3.0.3",
    "ng-apexcharts": "^1.7.1",
    "ng-let": "^15.0.1",
    "ngx-toastr": "^16.0.2",
    "path-to-regexp": "^6.2.1",
    "rxjs": "^7.8.0",
    "tslib": "^2.4.0",
    "zone.js": "^0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.3",
    "@angular-eslint/builder": "15.2.0",
    "@angular-eslint/eslint-plugin": "15.2.0",
    "@angular-eslint/eslint-plugin-template": "15.2.0",
    "@angular-eslint/schematics": "15.2.0",
    "@angular-eslint/template-parser": "15.2.0",
    "@angular/cli": "^15.1.3",
    "@angular/compiler-cli": "^15.1.2",
    "@openapitools/openapi-generator-cli": "^2.5.1",
    "@types/bootstrap": "^5.2.3",
    "@types/jasmine": "~4.3.0",
    "@types/jquery": "^3.5.14",
    "@types/luxon": "^3.0.1",
    "@types/node": "^18.7.14",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "bootstrap-icons": "^1.9.1",
    "cypress": "^12.4.1",
    "eslint": "^8.28.0",
    "jasmine-core": "^4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "start-server-and-test": "^1.15.3",
    "svg-sprite": "^1.5.4",
    "typescript": "~4.9.4"
  }
}
