{
  "name": "time-tracking",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run build:icons && npm run build:flags && ng serve --host 0.0.0.0 --disable-host-check",
    "start:api": "dotnet run --project ../FS.TimeTracking/FS.TimeTracking/FS.TimeTracking.csproj",
    "build": "npm run build:icons && npm run build:flags && ng build --source-map=true",
    "build:watch": "npm run build -- --watch --configuration development",
    "build:icons": "node icon-generator/create-sprite.js icons",
    "build:flags": "node icon-generator/create-sprite.js flags",
    "lint": "ng lint",
    "cypress": "npx cypress open",
    "test": "npm run test:unit:headless && npm run test:e2e",
    "test:unit": "ng test --browsers=Chrome",
    "test:unit:headless": "npm run test:unit -- --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "test:cypress": "npx cypress run",
    "test:e2e": "start-server-and-test start:api http-get://127.0.0.1:5000/openapi/index.html start http-get://127.0.0.1:4200 test:cypress"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.7",
    "@angular/common": "^17.3.7",
    "@angular/compiler": "^17.3.7",
    "@angular/core": "^17.3.7",
    "@angular/forms": "^17.3.7",
    "@angular/localize": "^17.3.7",
    "@angular/platform-browser": "^17.3.7",
    "@angular/platform-browser-dynamic": "^17.3.7",
    "@angular/router": "^17.3.7",
    "@ng-bootstrap/ng-bootstrap": "^14.1.0",
    "@ng-select/ng-select": "^10.0.4",
    "apexcharts": "^3.39.0",
    "bootstrap": "^5.2.3",
    "bootstrap-datepicker": "^1.9.0",
    "bootstrap-icons": "^1.10.4",
    "keycloak-js": "^21.0.2",
    "luxon": "^3.3.0",
    "ng-apexcharts": "^1.7.5",
    "ng-let": "^15.0.2",
    "ngx-toastr": "^16.1.1",
    "path-to-regexp": "^6.2.1",
    "rxjs": "^7.8.0",
    "tslib": "^2.5.0",
    "zone.js": "^0.14.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.6",
    "@angular-eslint/builder": "17.3.0",
    "@angular-eslint/eslint-plugin": "17.3.0",
    "@angular-eslint/eslint-plugin-template": "17.3.0",
    "@angular-eslint/schematics": "17.3.0",
    "@angular-eslint/template-parser": "17.3.0",
    "@angular/cli": "^17.3.6",
    "@angular/compiler-cli": "^17.3.7",
    "@openapitools/openapi-generator-cli": "^2.6.0",
    "@types/bootstrap": "^5.2.6",
    "@types/jasmine": "~4.3.1",
    "@types/jquery": "^3.5.16",
    "@types/luxon": "^3.3.0",
    "@types/node": "^18.16.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "bootstrap-icons": "^1.10.4",
    "cypress": "^12.10.0",
    "eslint": "^8.39.0",
    "jasmine-core": "^4.6.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "start-server-and-test": "^2.0.0",
    "svg-sprite": "^2.0.2",
    "typescript": "~5.4.5"
  }
}